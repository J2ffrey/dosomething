<script>

$(function(){
  
  $("#prev_").on("click",function(){
		$.ajax({
  			type:"get",
  			url:"/calendar/large?" + 'month=' + $('#month1').val()
  			+ '&year=' + $('#year1').val()
  			+ '&nocol=' + $('1').val()
  			+ '&day=' + $('#day1').val(),
  			datatype:"html",
  			success:function(r){
  				$("#large").empty();
  				$("#large").append(r);
  			},
  			error:function(){
  				alert("데이터를 가져오지 못하였습니다.");
  			}
  		})
  		return false;
    });
  
  $("#next_").on("click",function(){
		$.ajax({
  			type:"get",
  			url:"/calendar/large?" + 'month=' + $('#month2').val()
  			+ '&year=' + $('#year2').val()
  			+ '&nocol=' + $('1').val()
  			+ '&day=' + $('#day2').val(),
  			datatype:"html",
  			success:function(r){
  				$("#large").empty();
  				$("#large").append(r);
  			},
  			error:function(){
  				alert("데이터를 가져오지 못하였습니다.");
  			}
  		})
  		return false;
    })
  })
</script>